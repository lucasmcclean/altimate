{
	"manifest_version": 3,
	"name": "Altimate Accessibility",
	"description": "A Chrome extension to help with accessibility.",
	"version": "1.0",
	"icons": {
		"16": "logo.png",
		"32": "logo.png", 
		"48": "logo.png",
		"128": "logo.png"
	},

	"action": {
		"default_popup": "index.html",
		"default_title": "My Extension",
		"default_icon": {
			"16": "logo.png",
			"32": "logo.png",
			"48": "logo.png",
			"128": "logo.png"
		}
	},  "permissions": [
		"activeTab",
		"scripting",
		"tabs"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
